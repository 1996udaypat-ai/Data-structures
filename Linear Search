#include <stdio.h>

int main() {
    int n, key, found = 0, pos = -1;

    // Read size of the array
    scanf("%d", &n);

    int arr[n];

    // Read array elements
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Read the element to search
    scanf("%d", &key);

    // Linear search
    for (int i = 0; i < n; i++) {
        if (arr[i] == key) {
            found = 1;
            pos = i;
            break;
        }
    }

    // Output result
    if (found) {
        printf("found at position %d\n", pos);
    } else {
        printf("%d not found\n", key);
    }

    return 0;
}
